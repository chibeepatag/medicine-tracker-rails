<p id="notice"><%= notice %></p>

<h1>Organs</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @organs.each do |organ| %>
      <tr>
        <td><%= organ.id %></td>
        <td><%= organ.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
